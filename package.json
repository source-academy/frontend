{
  "private": true,
  "name": "cadet-frontend",
  "version": "0.0.0",
  "scripts-info": {
    "format": "Format source code",
    "start": "Start the Webpack development server",
    "build": "Build production bundle",
    "test": "Run unit tests",
    "update-ui-snapshots": "Update UI test snapshots"
  },
  "scripts": {
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:ci": "prettier --list-different 'src/**/*.{ts,tsx}'",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "update-ui-snapshots": "jest --updateSnapshot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && CI=true yarn test",
      "pre-push": "yarn format && CI=true yarn test"
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^2.1.1",
    "normalize.css": "^8.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-transition-group": "^2.3.1",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "typesafe-actions": "^1.1.2",
    "utility-types": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.10",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "@types/react-router": "^4.0.24",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-router-redux": "^5.0.13",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux-mock-store": "^0.0.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^1.0.0-rc.6",
    "prettier": "^1.12.0",
    "react-scripts-ts": "^2.15.1",
    "react-test-renderer": "^16.3.1",
    "typescript": "^2.8.1"
  }
}
